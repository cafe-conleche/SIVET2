        "servicio"
      ]
    },
    "cantidad": {
      "type": "integer",
      "default": 1
    },
    "precio_unitario": {
      "type": "decimal"
    },
    "descuento": {
      "type": "decimal"
    },
    "subtotal": {
      "type": "decimal"
    },
    "descripcion": {
