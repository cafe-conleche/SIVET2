      "inversedBy": "item_facturas"
    },
    "factura": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::factura.factura",
      "inversedBy": "item_facturas"
    },
    "producto": {
      "type": "relation",
      "relation": "manyToOne",
