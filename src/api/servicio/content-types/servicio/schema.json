{
  "kind": "collectionType",
  "collectionName": "servicios",
  "info": {
    "singularName": "servicio",
    "pluralName": "servicios",
    "displayName": "Servicio"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string"
    },
    "precio": {
      "type": "decimal"
    },
    "duracion_minutos": {
      "type": "integer"
    },
    "descripcion": {
      "type": "blocks"
    },
    "activo": {
      "type": "boolean",
      "default": true
    },
    "tipo_servicio": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tipo-servicio.tipo-servicio",
      "inversedBy": "servicios"
    },
    "citas": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::cita.cita",
      "mappedBy": "servicios"
    }
  }
}
